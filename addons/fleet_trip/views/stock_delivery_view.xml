        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="stock_delivery_menu"
              name="Phiếu xuất kho"
              parent="fleet_root_menu"
              action="stock_delivery_action"
              sequence="20"/> -->
</odoo>